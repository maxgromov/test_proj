#-------------Query----------------
type User{
    name:String
    id:Int
}
type Query{
    hello: String
    findUser(id:Int!): User!
}
#-------------Query---------------


#------------Mutations-------------
type UserCreateData{
    user: User
    error:String
}
type Mutation{
    userCreate(name: String!): UserCreateData
}
#------------Mutations-------------


schema {
    query: Query
    mutation: Mutation
}